    "webpack-prod": "webpack --config webpack.config.build.babel.js",
    "test": "npm run test:unit -- --coverage",
    "test:unit": "jest --config jest.config.js",
    "test:unit:watch": "npm run test:unit --watch",
    "test:e2e-dev": "testcafe \"chrome:headless\" __e2e__/app.spec.js",
    "test:e2e-uat": "cross-env BASE_URL=-uat.mindshareworld.com testcafe \"chrome:headless\" __e2e__/app.spec.js"
  },
